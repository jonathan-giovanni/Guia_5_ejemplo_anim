<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">

    <translate
        android:fromXDelta="100"
        android:toXDelta="0"
        android:fromYDelta="100"
        android:toYDelta="0"
        />
    <scale
        android:fromXScale="2"
        android:fromYScale="2"
        android:toYScale="1"
        android:toXScale="1"
        />
    <rotate
        android:fromDegrees="360"
        android:toDegrees="0"/>
    <alpha
        android:fromAlpha="1"
        android:toAlpha="0"/>

</set>